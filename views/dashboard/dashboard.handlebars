<h2>Panel de Administración</h2>
<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Email</th>
      <th>Acción</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
    <tr>
      <td>{{this.first_name}} {{this.last_name}}</td>
      <td>{{this.email}}</td>
      <td>
        <form action="/api/users/admin/delete/{{this._id}}" method="POST">
          <button type="submit">Eliminar</button>
        </form>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
